                    <th>Platforms</th>
                    <td>{{ log_book_cycle_report.platforms  | join(',')}}</td>
                </tr>
                <tr>
                    <th>Chips</th>
                    <td>{{ log_book_cycle_report.chips | join(',') }}</td>
                </tr>
                <tr>
                    <th>Mode</th>
                    <td>{{ log_book_cycle_report.mode }}</td>
                </tr>
                <tr>
                    <th>Components</th>
                    <td>{{ log_book_cycle_report.components | join(',') }}</td>
                </tr>
                <tr>
                    <th>URL</th>
                    <td>SELF_PATH &nbsp;&nbsp; Created:{{ log_book_cycle_report.createdAt ? log_book_cycle_report.createdAt|date('Y-m-d H:i:s') : '' }}
                        &nbsp;&nbsp; Updated:
                        {{ log_book_cycle_report.updatedAt ? log_book_cycle_report.updatedAt|date('Y-m-d H:i:s') : '' }}
                    </td>
                </tr>
                </tbody>
            </table>

        </td>
        <td style="width: 48%"><div id="piechart_verdicts" style="width: 900px; height: 500px;"></div></td>
    </tr>
</table>
