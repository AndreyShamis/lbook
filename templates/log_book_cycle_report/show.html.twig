    {% apply markdown_to_html %}{{ log_book_cycle_report.suitesNotes }}{% endapply %}
    <br/>
    <br/>
    <br/>



    <h5>Failed tests section</h5>
    <div>Count {{ testsNotPass | length }}</div>
    {% include "lbook/cycle/tests_not_pass.html.twig" %}
